{
	"manifest_version": 3,
	"name": "FreePhish",
	"version": "0.1.0",
	"description": "Blocks phishing websites.",
	"permissions": ["storage", "tabs", "scripting"],
	"host_permissions": ["*://*/*"],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_title": "FreePhish",
		"default_popup": "popup.html"
	},
	"icons": {
		"128": "icons/128.png",
		"64": "icons/64.png"
	}
}